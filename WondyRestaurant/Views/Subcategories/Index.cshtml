@model IEnumerable<WondyRestaurant.Models.SubCategory>
@{
    ViewData["Title"] = "Index2";
}
<br />
<h2> List Of Sub Categories</h2>
<br />

@Html.Partial("_CreateButtonPartial")

<div class="form-border">
    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(m => m.Name)
                </th>
                <th>
                    @Html.DisplayNameFor(m => m.Category.Name)
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
            <tr>
                <td> 
                    @Html.DisplayFor(m => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(m => item.Category.Name)
                </td>
                <td>
                    @Html.Partial("_TableButtonPartial", item.Id)
                </td>
               
            </tr>
            }
        </tbody>
    </table>
</div>



